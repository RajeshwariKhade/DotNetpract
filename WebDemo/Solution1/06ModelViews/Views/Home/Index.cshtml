
@{
    ViewData["Title"] = "Index";
}
<link href="~/lib/twitter-bootstrap/css/bootstrap.min.css" rel="stylesheet" />
<p class="text-bg-info">Index</p>
<div class="card" style="width: 18rem;">
    <img src="~/Images/Bickist.jpg" class="card-img-top" alt="Biskict">
    <div class="card-body">
        <h5 class="card-title">@Model.Product.PName</h5>
        <p class="card-text">@Model.Description</p>
        @if (@Model.isInStock)
        {
            <p class="card-text">in Stock</p>
        }
        else
        {
            <p class="card-text">out of Stock</p>
        }

        <a href="/Home/ProductDeatils/@Model.Product.PId" class="btn btn-primary">Product Details</a>
    </div>
</div>

<script src="~/twitter-bootstrap/js/bootstrap.min.js"></script>
<h1>Index</h1>


